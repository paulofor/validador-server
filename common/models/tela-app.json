{
  "name": "TelaApp",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nome": {
      "type": "string"
    },
    "tipo": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "itemValidacaoPaginas": {
      "type": "hasMany",
      "model": "ItemValidacaoPagina",
      "foreignKey": ""
    },
    "entidade": {
      "type": "belongsTo",
      "model": "entidade",
      "foreignKey": ""
    },
    "conceitoProduto": {
      "type": "belongsTo",
      "model": "ConceitoProduto",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {
    "ListaTelaAppProjeto": {
      "accepts": [
        {
          "arg": "idProjeto",
          "type": "number",
          "required": false,
          "description": ""
        },
        {
          "arg": "idConceito",
          "type": "number",
          "required": false,
          "description": ""
        }
      ],
      "returns": [
        {
          "arg": "listaTelaApp",
          "type": "array",
          "root": true,
          "description": ""
        }
      ],
      "description": "Retorna as telas de um projeto, pode ser pelo idProjeto ( conceito ativo ) ou pelo idConceito",
      "http": [
        {
          "path": "/listaTelaAppProjeto",
          "verb": "get"
        }
      ]
    },
    "ObtemPorIdAplicacaoParaGerador": {
      "accepts": [
        {
          "arg": "idAplicacao",
          "type": "number",
          "required": true,
          "description": ""
        }
      ],
      "returns": [
        {
          "arg": "listaTela",
          "type": "array",
          "root": true,
          "description": ""
        }
      ],
      "description": "Obtem as telas para serem criadas no gerador de codigo por idAplicacao",
      "http": [
        {
          "path": "/obtemPorIdAplicacaoParaGerador",
          "verb": "get"
        }
      ]
    }
  }
}
